					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "demo.csv",
						"folderPath": "source",
						"fileSystem": {
							"value": "@dataset().container",
							"type": "Expression"
						}
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
